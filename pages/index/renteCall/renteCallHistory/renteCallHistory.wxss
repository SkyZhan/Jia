/* pages/index/renteCall/renteCallHistory/renteCallHistory.wxss */
page{
  width: 100%；
}
.container{

display: flex;
flex-direction: column;
padding: 0;
box-sizing: border-box;
}
.returnBtn{
  position: fixed;
  top: 6rpx;
  left: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background: rgba(0,0,0,0.4); 
  border-radius:100rpx; 
  padding-top: 6rpx;
}
.returnBtn image{
  width: 28rpx;
  height: 33rpx;
  transform:rotate(180deg); 
  margin-left: 14rpx;
  
}

.history-table-wrap{
  width: 95%;
  height: 578rpx;
  margin-top: 10rpx;

}
/* 表格代码   */
.table{
  border:1px solid #dadada;
  border-radius:25rpx; 
  width: 98%;
  margin-left: 1%;
  margin-bottom: 10rpx;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
}
.th{
  padding: 6rpx;
  text-align: center;
  width: 100%;
  color: #330e0e;
  font-size: 28rpx;
}
.td{
  padding: 6rpx;
  text-align: left;
  width: 100%;
  color:#ec9480;
  font-size: 28rpx;
}
.th1{
  width: 25%;
}
.sumstyle{
  color:#09bb07 ;
  font-size:34rpx; 
}
